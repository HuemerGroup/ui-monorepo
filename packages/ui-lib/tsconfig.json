{
  "extends": "../../tsconfig.json",
  "include": [
    "./src"
  ],
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "outDir": "./build",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  }
}
